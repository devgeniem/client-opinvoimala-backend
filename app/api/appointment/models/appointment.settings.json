{
  "kind": "collectionType",
  "collectionName": "appointments",
  "info": {
    "name": "appointment",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "meeting_link": {
      "type": "string",
      "configurable": false
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "available",
        "booked",
        "cancelled",
        "hidden"
      ],
      "default": "available"
    },
    "start_time": {
      "type": "datetime",
      "required": true
    },
    "end_time": {
      "type": "datetime",
      "required": true
    },
    "user": {
      "private": true,
      "plugin": "users-permissions",
      "model": "user",
      "via": "appointments"
    },
    "appointment_specialist": {
      "model": "appointment-specialist"
    }
  }
}
